---
layout: null
permalink: /sitemap.xml
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- assign pages = site.pages | where_exp: "p", "p.sitemap != false" -%}
  {%- for page in pages -%}
    {%- if page.url and page.url != "/404.html" -%}
  <url>
    <loc>{{ site.url }}{{ page.url | replace: "index.html", "" }}</loc>
    <lastmod>{{ page.last_modified_at | default: page.date | default: site.time | date_to_xmlschema }}</lastmod>
    <changefreq>{% if page.url == "/" %}weekly{% else %}monthly{% endif %}</changefreq>
    <priority>{% if page.url == "/" %}1.0{% else %}0.5{% endif %}</priority>
  </url>
    {%- endif -%}
  {%- endfor -%}

  <!-- Product properties on subdomains -->
  <url>
    <loc>https://lovelink.merchandice.in/</loc>
    <changefreq>yearly</changefreq>
    <priority>0.6</priority>
  </url>
  <url>
    <loc>https://memorywall.merchandice.in/</loc>
    <changefreq>yearly</changefreq>
    <priority>0.6</priority>
  </url>
  <url>
    <loc>https://lovemeter.merchandice.in/</loc>
    <changefreq>yearly</changefreq>
    <priority>0.6</priority>
  </url>
</urlset>

